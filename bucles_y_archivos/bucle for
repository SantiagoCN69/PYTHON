# Ejemplo de bucle for en Python
for i in range(1, 11):
    print(f"El número es: {i}")

# Bucle for recorriendo una lista
frutas = ["manzana", "banana", "cereza", "naranja"]
for fruta in frutas:
    print(f"Me gusta la {fruta}")

# Bucle for con enumerate
for indice, fruta in enumerate(frutas):
    print(f"Fruta número {indice + 1}: {fruta}")
